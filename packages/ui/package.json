{
  "name": "@spark-ui/ui",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Spark (Leboncoin design system) components.",
  "exports": {
    "./*": {
      "types": "./dist/*/index.d.ts",
      "import": "./dist/*/index.mjs",
      "require": "./dist/*/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*/index.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "@spark-ui",
    "react",
    "component",
    "accessible",
    "accessibility",
    "wai-aria",
    "aria",
    "a11y"
  ],
  "scripts": {
    "build": "NODE_OPTIONS='--max-old-space-size=8192' tsup"
  },
  "dependencies": {
    "@spark-ui/icons": "*",
    "@spark-ui/internal-utils": "^9.4.8",
    "@spark-ui/use-merge-refs": "^9.4.8",
    "@spark-ui/use-combined-state": "^9.4.8",
    "@zag-js/accordion": "1.4.1",
    "@zag-js/react": "1.4.1",
    "@zag-js/collapsible": "1.4.1",
    "@zag-js/pagination": "1.4.1",
    "@react-aria/toast": "^3.0.0-beta.18",
    "@react-aria/button": "3.12.1",
    "@react-aria/numberfield": "3.11.12",
    "@react-stately/toast": "3.0.0-beta.7",
    "@react-stately/numberfield": "3.9.10",
    "class-variance-authority": "0.7.1",
    "radix-ui": "1.1.3",
    "downshift": "9.0.8",
    "react-snap-carousel": "^0.5.1",
    "emulate-tab": "^1.2.1"
  },
  "peerDependencies": {
    "@spark-ui/theme-utils": "latest",
    "react": "^19.0",
    "react-dom": "^19.0",
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "jsdom-testing-mocks": "1.13.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leboncoin/spark-web.git",
    "directory": "packages/ui"
  },
  "bugs": {
    "url": "https://github.com/leboncoin/spark-web/issues?q=is%3Aopen+label%3A%22Component%3A+button%22"
  },
  "homepage": "https://sparkui.vercel.app"
}
